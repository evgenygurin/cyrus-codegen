{
	"name": "cyrus-codegen-integration",
	"version": "0.1.0",
	"description": "TypeScript Codegen API integration for Cyrus",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"typecheck": "tsc --noEmit",
		"lint": "biome check src",
		"format": "biome check --write --unsafe src"
	},
	"dependencies": {
		"@anthropic-ai/claude-agent-sdk": "^0.1.60",
		"axios": "^1.6.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.3",
		"@types/node": "^20.0.0",
		"@types/react": "^18.2.0",
		"@vitest/coverage-v8": "^1.1.0",
		"react": "^18.2.0",
		"typescript": "^5.3.3",
		"vitest": "^1.1.0"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"codegen",
		"typescript",
		"api-client",
		"webhook",
		"agent",
		"cyrus"
	]
}
